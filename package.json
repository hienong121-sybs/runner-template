{
  "name": "runner-template-copy",
  "version": "0.7.0",
  "description": "CLI tools to copy runner-template files, create Cloudflare tunnels, and update Tailscale access controls",
  "bin": {
    "runner-template": "./runner-template.js",
    "runner-template-copy": "./runner-template-copy.js",
    "runner-template-createtunnel": "./runner-template-createtunnel.js",
    "runner-template-tailscale": "./runner-template-tailscale.js"
  },
  "license": "MIT",
  "files": [
    "runner-template.js",
    "runner-template-copy.js",
    "runner-template-createtunnel.js",
    "runner-template-tailscale.js",
    ".bash/.bash_aliase",
    ".bash/setup-bash-aliase.sh",
    "docker-compose.yml",
    ".docker-manager/.gitkeep",
    "docker-manager/**/*",
    "caddy/Caddyfile",
    "caddy/entrypoint.sh",
    "nginx/conf.d/app.conf.template",
    "nginx/maps/mirror_rules.map",
    "nginx/shadow-servers/.gitkeep",
    "nginx/scripts/shadow-add.sh",
    "nginx/scripts/shadow-rm.sh",
    "nginx/scripts/shadow-sync.sh",
    "nginx/setup-htpasswd.sh",
    "nginx/upstreams/main_upstream.conf.template",
    "nginx/upstreams/shadow_upstream.conf.template",
    "scripts/pull-data.js",
    "scripts/setup-runner-helper.js",
    "scripts/setup-runner-prev.js",
    "scripts/setup-runner-after.js",
    "tailscale/**/*",
    "README.md"
  ]
}
